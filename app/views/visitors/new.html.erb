<% content_for :title do %>Webinar Executive Coaching<% end %>
<% content_for :description do %>Webinar Executive Coaching<% end %>
<section>
  <%= image_tag("proto3_3_1.png") %>
</section>
<section>
  <div class="column">
    <h2>Stay in touch.</h2>
  </div>
  <div class="column">
    <div class="form-centered">
      <%= simple_form_for @visitor do |f| %>
        <%= f.error_notification %>
        <%= f.input :email, label: false, :placeholder => 'Your email address...' %>
        <br/>
        <%= f.button :submit, "Sign up for the newsletter", :class => "submit" %>
      <% end %>
    </div>
  </div>
</section>
