<h1>Listing webinars</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Subject</th>
      <th>Detail</th>
      <th>Presenter</th>
      <th>Info</th>
      <th>Url</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @webinars.each do |webinar| %>
      <tr>
        <td><%= webinar.date %></td>
        <td><%= webinar.subject %></td>
        <td><%= webinar.detail %></td>
        <td><%= webinar.presenter %></td>
        <td><%= webinar.info %></td>
        <td><%= link_to webinar["url"], webinar["url"] %></td>
        <td><%= link_to 'Show', webinar %></td>
        <td><%= link_to 'Edit', edit_webinar_path(webinar) %></td>
        <td><%= link_to 'Destroy', webinar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Webinar', new_webinar_path %>
